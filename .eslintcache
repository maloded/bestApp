rc\\redux\\redux-store.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Profile\\MyPost\\MyPostContainer.jsx",["139"],"import React from 'react';\r\nimport { connect } from 'react-redux';\r\nimport { addPost, updatePostText } from '../../../../redux/profile-reducer';\r\nimport MyPost from './MyPost';\r\n\r\n\r\nlet mapStateToProps = (state) => {\r\n    return {\r\n        posts: state.profilePage.posts,\r\n        newPostText: state.profilePage.newPostText\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps, {addPost, updatePostText})(MyPost);","D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Users\\UsersContainer.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\users-reducer.js",["140"],"import { followAPI, usersAPI } from '../API/api';\r\n\r\nconst FOLLOW = 'FOLLOW';\r\nconst UNFOLLOW = 'UNFOLLOW';\r\nconst SET_USERS = 'SET-USERS';\r\nconst SET_TOTAL_USERS_COUNT = 'SET-TOTAL-USERS-COUNT';\r\nconst SET_CURRENT_PAGE = 'SET-CURRENT-PAGE';\r\nconst TOGGLE_IS_FETCHING = 'TOGGLE-IS-FETCHING';\r\nconst TOGGLE_IS_FOLLOWING_PROGRESS = 'TOGGLE-IS-FOLLOWING-PROGRESS';\r\n\r\nlet initialState = {\r\n    users: [],\r\n    pagesSize: 10,\r\n    totalUsersCount: 0,\r\n    currentPage: 1,\r\n    isFetching: false,\r\n    followingInProgress: []\r\n}\r\n\r\nconst usersReducer = (state = initialState, action) => {\r\n    switch (action.type) {\r\n        case FOLLOW: {\r\n            return {...state, users: state.users.map(u => {\r\n                if (u.id === action.userId) {\r\n                    return {...u, followed: true}\r\n                }\r\n                return u;\r\n            })}\r\n        }\r\n        case UNFOLLOW: {\r\n            return {...state, users: state.users.map(u => {\r\n                if (u.id === action.userId) {\r\n                    return {...u, followed: false}\r\n                }\r\n                return u;\r\n            })}\r\n        }\r\n        case SET_USERS: {\r\n            return {...state, users: action.users}\r\n        }\r\n        case SET_TOTAL_USERS_COUNT: {\r\n            return {...state, totalUsersCount: action.totalCount}\r\n        }\r\n        case SET_CURRENT_PAGE: {\r\n            return {...state, currentPage: action.currentPage}\r\n        }\r\n        case TOGGLE_IS_FETCHING: {\r\n            return {...state, isFetching: action.isFetching}\r\n        }\r\n        case TOGGLE_IS_FOLLOWING_PROGRESS: {    \r\n            return {...state, \r\n                followingInProgress: action.isFollowingFetching \r\n                ? [...state.followingInProgress, action.userId]\r\n                : state.followingInProgress.filter(id => id != action.userId)\r\n            }\r\n        }\r\n        default: \r\n            return state;\r\n    }\r\n}\r\n\r\nexport const followSaccess = (userId) => ({type: FOLLOW, userId})\r\nexport const unfollowSaccess = (userId) => ({type: UNFOLLOW, userId})\r\nexport const setUsers = (users) => ({type: SET_USERS, users})\r\nexport const setTotalUsersCount = (totalCount) => ({type: SET_TOTAL_USERS_COUNT, totalCount})\r\nexport const setCurrentPage = (currentPage) => ({type: SET_CURRENT_PAGE, currentPage})\r\nexport const isUsersFetching = (isFetching) => ({type: TOGGLE_IS_FETCHING, isFetching})\r\nexport const toggleIsFollowingProgress = (isFollowingFetching, userId) => ({type: TOGGLE_IS_FOLLOWING_PROGRESS, isFollowingFetching, userId})\r\n\r\nexport const follow = (userId) => {\r\n    return (dispatch) => {\r\n        dispatch(toggleIsFollowingProgress(true, userId))\r\n        followAPI.followUser(userId).then(data => {\r\n            if (data.resultCode === 0) {\r\n                dispatch(followSaccess(userId));\r\n            }\r\n        });\r\n        dispatch(toggleIsFollowingProgress(false, userId));\r\n    }\r\n}\r\n\r\nexport const unfollow = (userId) => {\r\n    return (dispatch) => {\r\n        dispatch(toggleIsFollowingProgress(true, userId));\r\n        followAPI.unfollowUser(userId).then(data => {\r\n            if (data.resultCode === 0) {\r\n                dispatch(unfollowSaccess(userId));\r\n            }\r\n        });\r\n        dispatch(toggleIsFollowingProgress(false, userId));\r\n    }\r\n}\r\n\r\nexport const getUsersPage = (currentPage, pageSize) => {\r\n    return (dispatch) => {\r\n        dispatch(setCurrentPage(currentPage));\r\n        dispatch(isUsersFetching(true));\r\n        usersAPI.getUsers(currentPage, pageSize).then(data=> {\r\n            dispatch(setUsers(data.items));\r\n            dispatch(setTotalUsersCount(data.totalCount));\r\n            dispatch(isUsersFetching(false));\r\n        });\r\n    }   \r\n}\r\n\r\n\r\nexport default usersReducer;\r\n\r\n\r\n\r\n","D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\common\\Prelouder.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Profile\\ProfileContainer.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\auth-reducer.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Header\\HeaderContainer.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\API\\api.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Login\\Login.jsx",["141"],"D:\\React\\Projects\\second_project\\my-best-app\\src\\hoc\\withAuthRedirect.jsx",[],{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","severity":1,"message":"147","line":3,"column":8,"nodeType":"148","messageId":"149","endLine":3,"endColumn":15},{"ruleId":"146","severity":1,"message":"150","line":2,"column":8,"nodeType":"148","messageId":"149","endLine":2,"endColumn":15},{"ruleId":"142","replacedBy":"151"},{"ruleId":"144","replacedBy":"152"},{"ruleId":"146","severity":1,"message":"150","line":3,"column":8,"nodeType":"148","messageId":"149","endLine":3,"endColumn":15},{"ruleId":"146","severity":1,"message":"150","line":3,"c