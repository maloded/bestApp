onent={Textarea} validate={[required, maxLength70]} name='newPostText' placeholder='Text post' />\r\n            </div>\r\n            <div>\r\n                <button>add post</button>\r\n            </div>\r\n        </form>\r\n    )\r\n}\r\n\r\nconst AddMyPostForm = reduxForm({form: 'profile'})(AddMyPost)\r\n\r\nconst MyPost = (props) => {\r\n    \r\n    let onAddPost = (value) => {\r\n        props.addPost(value.newPostText)\r\n    }\r\n\r\n    let postsElements = props.posts.map(p => <Posts text={p.text} likesCount={p.likesCount} />)\r\n\r\n    return (\r\n        <div>\r\n            <div>\r\n                <AddMyPostForm onSubmit={onAddPost} />\r\n            </div>\r\n            { postsElements }    \r\n        </div>       \r\n    );\r\n}\r\nexport default MyPost;","D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Sitebar\\Navbar\\Navbar.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Profile\\MyPost\\Posts\\Posts.jsx",["154"],"import React from 'react';\r\nimport classes from './Posts.module.css';\r\n\r\nconst Posts = (props) => {\r\n    return (\r\n        <div>\r\n            <div>\r\n                <img src=\"2\" alt=\"img\"/>\r\n            </div>\r\n            <div>\r\n                {props.text}\r\n            </div>\r\n            <div>\r\n            {props.likesCount + ' likes'}\r\n            </div>\r\n        </div>       \r\n    );\r\n}\r\n\r\nexport default Posts;","D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Dialogs\\DialogItem\\DialogItem.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Dialogs\\MessageItem\\MessageItem.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\store.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\profile-reducer.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\dialogs-reducer.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\redux-store.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Users\\UsersContainer.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\users-reducer.js",["155"],"import { followAPI, usersAPI } from '../API/api';\r\n\r\nconst FOLLOW = 'FOLLOW';\r\nconst UNFOLLOW = 'UNFOLLOW';\r\nconst SET_USERS = 'SET-USERS';\r\nconst SET_TOTAL_USERS_COUNT = 'SET-TOTAL-USERS-COUNT';\r\nconst SET_CURRENT_PAGE = 'SET-CURRENT-PAGE';\r\nconst TOGGLE_IS_FETCHING = 'TOGGLE-IS-FETCHING';\r\nconst TOGGLE_IS_FOLLOWING_PROGRESS = 'TOGGLE-IS-FOLLOWING-PROGRESS';\r\n\r\nlet initialState = {\r\n    users: [],\r\n    pagesSize: 10,\r\n    totalUsersCount: 0,\r\n    currentPage: 1,\r\n    isFetching: false,\r\n    followingInProgress: []\r\n}\r\n\r\nconst usersReducer = (state = initialState, action) => {\r\n    switch (action.type) {\r\n        case FOLLOW: {\r\n            return {...state, users: state.users.map(u => {\r\n                if (u.id === action.userId) {\r\n                    return {...u, followed: true}\r\n                }\r\n                return u;\r\n            })}\r\n        }\r\n        case UNFOLLOW: {\r\n            return {...state, users: state.users.map(u => {\r\n                if (u.id === action.userId) {\r\n                    return {...u, followed: false}\r\n                }\r\n                return u;\r\n            })}\r\n        }\r\n        case SET_USERS: {\r\n            return {...state, users: action.users}\r\n        }\r\n        case SET_TOTAL_USERS_COUNT: {\r\n            return {...state, totalUsersCount: action.totalCount}\r\n        }\r\n        case SET_CURRENT_PAGE: {\r\n            return {...state, currentPage: action.currentPage}\r\n        }\r\n        case TOGGLE_IS_FETCHING: {\r\n            return {...state, isFetching: action.isFetching}\r\n        }\r\n        case TOGGLE_IS_FOLLOWING_PROGRESS: {    \r\n            return {...state, \r\n                followingInProgress: action.isFollowingFetching \r\n                ? [...state.followingInProgress, action.userId]\r\n                : state.followingInProgress.filter(id => id != action.userId)\r\n            }\r\n        }\r\n        default: \r\n            return state;\r\n    }\r\n}\r\n\r\nexport const followSaccess = (userId) => ({type: FOLLOW, userId})\r\nexport const unfollowSaccess = (userId) => ({type: UNFOLLOW, userId})\r\nexport const setUsers = (users) => ({type: SET_USERS, users})\r\nexport const setTotalUsersCount = (totalCount) => ({type: SET_TOTAL_USERS_COUNT, totalCount})\r\nexport const setCurrentPage = (currentPage) => ({type: SET_CURRENT_PAGE, currentPage})\r\nexport const isUsersFetching = (isFetching) => ({type: TOGGLE_IS_FETCHING, isFetching})\r\nexport const toggleIsFollowingProgress = (isFollowingFetching, userId) => ({type: TOGGLE_IS_FOLLOWING_PROGRESS, isFollowingFetching, userId})\r\n\r\nexport const follow = (userId) => {\r\n    return (dispatch) => {\r\n        dispatch(toggleIsFollowingProgress(true, userId))\r\n        followAPI.followUser(userId).then(data => {\r\n            if (data.resultCode === 0) {\r\n                dispatch(followSaccess(userId));\r\n            }\r\n        });\r\n        dispatch(toggleIsFollowingProgress(false, userId));\r\n    }\r\n}\r\n\r\nexport const unfollow = (userId) => {\r\n    return (dispatch) => {\r\n        dispatch(toggleIsFollowingProgress(true, userId));\r\n        followAPI.unfollowUser(userId).then(data => {\r\n            if (data.resultCode === 0) {\r\n                dispatch(unfollowSaccess(userId));\r\n            }\r\n        });\r\n        dispatch(toggleIsFollowingProgress(false, userId));\r\n    }\r\n}\r\n\r\nexport const getUsersPage = (currentPage, pageSize) => {\r\n    return (dispatch) => {\r\n        dispatch(setCurrentPage(currentPage));\r\n        dispatch(isUsersFetching(true));\r\n        usersAPI.getUsers(currentPage, pageSize).then(data=> {\r\n            dispatch(setUsers(data.items));\r\n            dispatch(setTotalUsersCount(data.totalCount));\r\n            dispatch(isUsersFetching(false));\r\n        });\r\n    }   \r\n}\r\n\r\n\r\nexport default usersReducer;\r\n\r\n\r\n\r\n","D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\common\\Prelouder.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Profile\\ProfileContainer.jsx",["156"],"D:\\React\\Projects\\second_project\\my-best-app\\src\\redux\\auth-reducer.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Header\\HeaderContainer.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\API\\api.js",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Login\\Login.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\hoc\\withAuthRedirect.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Profile\\ProfileInfo\\ProfileStatus.jsx",[],"D:\\React\\Projects\\second_project\\my-best-app\\src\\components\\Content\\Dialogs\\InputMessageForm\\InputMessageForm.jsx",["157"],"import React from 'react';\r\nimport classes from './InputMessageForm.module.css';\r\nimport {Field, reduxForm} from 'redux-form';\r\nimport { Textarea } from '../../../common/FormsControl/FormsControl';\r\nimport { maxLengthCreator, required } from '../../../../utilities/validators';\r\n\r\nlet maxLength50 = maxLengthCreator(50)\r\n\r\nconst InputMessage = (props) => {\r\n    return (\r\n        <form onSubmit={props.handleSubmit}>\r\n            <div>\r\n                <Field component={Textarea} validate={[required, maxLength50]} name='newMessageText' placeholder='Text message' />\r\n            </div>\r\n            <div>\r\n                <button>add message</button>\r\n            </div>\r\n        </form>\r\n    )\r\n}\r\n\r\nconst InputMessageForm = reduxForm({form: 'dialog'})(InputMessage)\r\n\r\nexport default InputMessageForm;","D:\\React\\Projects\\second_pro